page{
    background-color: #262628;
}
.game-list-item{
    height: 400rpx;
    position: relative;
}
.game-list-item image{
    height: 400rpx;
    width: 100%;
}
.game-list-common{
    position: absolute;
    font-size: 28rpx;
    color: #ffffff;
    text-align: center;
}
.game-list-name{
    top: 42%;
    width: 100%;
}
.game-list-dec{
   top: 57%;
    width: 100%;
}

.themedetail-video{
}
.themedetail-dec{
   font-size: 26rpx;
   color: #ffffff;
   line-height: 36rpx;
   padding: 40rpx 20rpx 0;
}
.detail-video-bg{
    height: 420rpx;
    width: 100%;
    position: relative;
}
.detail-video-bg image{
    height: 100rpx;
    width: 100rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -50rpx;
    margin-left: -50rpx;
}
.detail-videolist{
    position: relative;
    height: 420rpx;
    width: 100%;
}
.wonderful-name{
    font-size: 24rpx;
    color: #ffffff;
    text-align: center;
    height: 80rpx;
    line-height: 80rpx;
}
.detail-video{
   
   height: 420rpx;
   width: 100%;
}
.themedetail-video-list{
    padding: 0 0 60rpx;
}
.themedetail-video-list:nth-of-type(1){
     padding: 0px 0 50rpx;
}
.themedetail-video-title{
    font-size: 34rpx;
    color: #ffffff;
    padding-top: 50rpx;
    text-align: center;
    font-weight: bold;
}
.themedetail-video-chan{
    font-size: 24rpx;
    color: #969696;
    padding-top: 16rpx;
    text-align: center;
}
.themedetail-video-dec{
    font-size: 24rpx;
    color: #969696;
    line-height: 40rpx;
    padding: 30rpx 20rpx 0;
    text-align: center;
}
.themedetail-img{
    position: relative;
     height: 250rpx;
     width: 100%;
}
.themedetail-img image{
   height: 250rpx;
   width: 100%;
}
.today-game-title{
    position: absolute;
    top: 45%;
    height: 40rpx;
    margin-top: -20rpx;
    line-height: 40rpx;
    font-size: 50rpx;
    color: #ffffff;
    width: 100%;
    text-align: center;
    font-family:'Square 721';  
}
.today-game-name{
    position: absolute;
    top: 62%;
    height: 40rpx;
    margin-top: -20rpx;
    line-height: 40rpx;
    font-size: 24rpx;
    color: #ffffff;
    width: 100%;
    text-align: center;
}
@font-face {
	font-family: 'Square 721';
	src: url('Square721BT-BoldExtended.eot');
	font-weight: bold;
	font-style: normal;
}
@font-face {
	font-family: 'Square 721';
	src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
		url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
		url('Square721BT-BoldExtended.ttf') format('truetype'),
		url('Square721BT-BoldExtended.svg#Square721BT-BoldExtended') format('svg');
	font-weight: bold;
	font-style: normal;
}